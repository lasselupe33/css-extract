{
  "name": "@css-extract/core",
  "version": "0.0.0",
  "main": "lib/index.mjs",
  "types": "src/index.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "scripts": {
    "eslint": "eslint --cache --no-error-on-unmatched-pattern --max-warnings 20 'src/**/*.{js,jsx,ts,tsx,mjs}'",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "dev": "npx rollup -c rollup.config.js --watch",
    "build": "npx rollup -c rollup.config.js"
  },
  "dependencies": {
    "@css-extract/utils": "0.0.0"
  },
  "devDependencies": {
    "@tooling/typescript-config": "*",
    "@tooling/rollup-config": "*",
    "@tooling/eslint-config": "*"
  }
}
