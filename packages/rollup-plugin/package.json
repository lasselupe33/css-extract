{
  "name": "@css-extract/rollup-plugin",
  "version": "0.0.0",
  "main": "lib/index.js",
  "types": "src/index.ts",
  "sideEffects": false,
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "license": "MIT",
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "scripts": {
    "eslint": "eslint --cache --no-error-on-unmatched-pattern --max-warnings 20 'src/**/*.{js,jsx,ts,tsx,mjs}'",
    "type-check": "tsc -p tsconfig.json --noEmit",
    "dev": "npx rollup -c rollup.config.js --watch",
    "build": "npx rollup -c rollup.config.js"
  },
  "dependencies": {
    "@css-extract/prepare": "*",
    "magic-string": "^0.30.2",
    "rollup": "^3.28.0",
    "postcss": "^8.4.27",
    "postcss-preset-env": "^9.1.1"
  },
  "devDependencies": {
    "@tooling/typescript-config": "*",
    "@tooling/rollup-config": "*",
    "@tooling/eslint-config": "*",
    "@types/node": "^18.17.1"
  }
}
